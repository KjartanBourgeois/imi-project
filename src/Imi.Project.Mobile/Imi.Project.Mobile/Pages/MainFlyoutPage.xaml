<?xml version="1.0" encoding="utf-8" ?>
<FlyoutPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Imi.Project.Mobile.Pages.MainFlyoutPage"
             xmlns:local="clr-namespace:Imi.Project.Mobile" xmlns:local1="clr-namespace:Imi.Project.Mobile.Pages"
            BackgroundColor="{StaticResource ColorPrimary}"
            NavigationPage.HasNavigationBar="False">
    <FlyoutPage.Flyout>
        <local1:MainFlyoutPageFlyout x:Name="flyoutPage" />
    </FlyoutPage.Flyout>
    <FlyoutPage.Detail>
        <NavigationPage>
            <x:Arguments>
                <local1:AllRecipesPage />
            </x:Arguments>
        </NavigationPage>
    </FlyoutPage.Detail>
</FlyoutPage>